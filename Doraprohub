Tab6:AddButton({
	Name = "📦 Xem Stock Trái Ác Quỷ",
	Callback = function()
		local stock = game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("GetFruits")
		local msg = "🎯 Stock Trái Ác Quỷ hôm nay:\n\n"
		for i,v in pairs(stock) do
			if v["Stock"] then
				msg = msg .. "- 🍇 " .. v["Name"] .. " - Giá: $" .. v["Price"] .. "\n"
			end
		end
		game.StarterGui:SetCore("SendNotification", {
			Title = "Stock Trái Ác Quỷ",
			Text = msg,
			Duration = 10
		})
	end
})


Tab4:AddToggle({
	Name = "🌿Auto Random Fruit",
	Default = false,
	Description = "Hỗ trợ bởi RedzHub",
	Callback = function(Value)
		_G.AutoRandomFruit = Value
		while _G.AutoRandomFruit do
			task.wait(5)
			pcall(function()
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Cousin")
			end)
		end
	end
})

Tab4:AddParagraph(" ---⚡Raid --- ", "")

_G.AutoRaid = false
Tab4:AddToggle({
	Name = "⚔️ Tự động đi Raid",
	Default = false,
	Callback = function(v)
		_G.AutoRaid = v
		while _G.AutoRaid and task.wait(3) do
			pcall(function()
				local plr = game.Players.LocalPlayer
				local placeId = game.PlaceId

				if placeId == 7449423635 then -- Sea 3
					local raidNpc = workspace.Map:FindFirstChild("Code") or workspace.Map:FindFirstChild("RaidLab") or workspace:FindFirstChild("RaidLab")
					if raidNpc then
						plr.Character.HumanoidRootPart.CFrame = raidNpc.CFrame + Vector3.new(0, 5, 0)
						wait(1)

						fireclickdetector(raidNpc:FindFirstChildWhichIsA("ClickDetector"))
						wait(1)

						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("RaidTeleport")
					end
				else
					OrionLib:MakeNotification({
						Name = "⚠️ Chỉ hỗ trợ Sea 3!",
						Content = "Tính năng Auto Raid chỉ hoạt động ở Sea 3 (Third Sea)",
						Image = "rbxassetid://6031280882",
						Time = 8
					})
				end
			end)
		end
	end
})


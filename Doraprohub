Tab8:AddButton({
	Name = "📟 Hiện thông tin nhân vật",
	Callback = function()
		local plr = game.Players.LocalPlayer
		local level = plr.Data.Level.Value
		local beli = plr.Data.Beli.Value
		local frags = plr.Data.Fragments.Value
		local sea = "Không rõ"

		local placeId = game.PlaceId
		if placeId == 2753915549 then
			sea = "🌊 Sea 1"
		elseif placeId == 4442272183 then
			sea = "🌊 Sea 2"
		elseif placeId == 7449423635 then
			sea = "🌊 Sea 3"
		end

		OrionLib:MakeNotification({
			Name = "📖 Thông tin nhân vật",
			Content = "Level: "..level.."\n💰 Beli: "..beli.."\n🔯 Fragments: "..frags.."\nSea hiện tại: "..sea,
			Image = "rbxassetid://4483345998",
			Time = 7
		})
	end
})

Tab8:AddButton({
	Name = "📌 Hiện chỉ số Stat",
	Callback = function()
		local plr = game.Players.LocalPlayer
		local melee = plr.Data.Stats.Melee.Level.Value
		local defense = plr.Data.Stats.Defense.Level.Value
		local sword = plr.Data.Stats.Sword.Level.Value
		local gun = plr.Data.Stats.Gun.Level.Value
		local fruit = plr.Data.Stats["Demon Fruit"].Level.Value

		OrionLib:MakeNotification({
			Name = "📖 Chỉ số Stat",
			Content = "Melee: "..melee.." | Defense: "..defense.."\nSword: "..sword.." | Gun: "..gun.."\nBlox Fruit: "..fruit,
			Image = "rbxassetid://6031280882",
			Time = 7
		})
	end
})


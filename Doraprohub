local AutoStatMelee = false
Tab2:AddToggle({
	Name = "♠️Melee",
	Default = false,
	Callback = function(Value)
		AutoStatMelee = Value
		while AutoStatMelee do
			task.wait(0.1)
			pcall(function()
				game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AddPoint", "Melee", 1)
			end)
		end
	end
})

